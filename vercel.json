{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node@3.1.0"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}