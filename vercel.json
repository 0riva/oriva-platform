{
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "includeFiles": "api/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "outputDirectory": "public"
}