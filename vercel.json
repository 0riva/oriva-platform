{
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "outputDirectory": "public",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ]
}